#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules/@upstash/context7-mcp/dist/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules/@upstash/context7-mcp/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules/@upstash/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules/@upstash/context7-mcp/dist/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules/@upstash/context7-mcp/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules/@upstash/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/@upstash+context7-mcp@1.0.16/node_modules:/Users/jack/IRONFORGE/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
